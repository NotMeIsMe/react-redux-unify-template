{
  "name": "react-redux-unify-template",
  "version": "0.1.0",
  "description": "template for reactjs redux, unify",
  "main": "src/server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/2637309949/react-redux-unify-template.git"
  },
  "keywords": [
    "express",
    "react",
    "redux",
    "template",
    "unify",
    "node"
  ],
  "author": "黄建双",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/"
  },
  "homepage": "https://github.com/",
  "scripts": {
    "eslint": "eslint -c .eslintrc src api bin",
    "dev": "concurrent --kill-others \"better-npm-run watch-client\" \"npm run start-dev\"",
    "pro": "npm run build && npm run start-pro",
    "start-dev": "better-npm-run start:dev",
    "start-pro": "better-npm-run start:pro",
    "build": "rimraf dist && webpack --config webpack/webpack.pro.config --progress --colors",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "start:dev": {
      "command": "node bin/main",
      "env": {
        "NODE_ENV": "development",
        "HOST": "127.0.0.1",
        "DEVPORT": 3001,
        "PORT": 3000
      }
    },
    "start:pro": {
      "command": "node bin/main",
      "env": {
        "NODE_ENV": "production",
        "HOST": "127.0.0.1",
        "DEVPORT": 3001,
        "PORT": 8080
      }
    },
    "watch-client": {
      "command": "node webpack/webpack.dev.server.js",
      "env": {
        "NODE_ENV": "development",
        "HOST": "127.0.0.1",
        "DEVPORT": 3001
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "dependencies": {
    "babel-register": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "express": "^4.14.1",
    "express-device": "^0.4.2",
    "log4js": "^1.1.0",
    "onsenui": "^2.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-onsenui": "^1.1.0",
    "react-redux": "^4.0.0",
    "react-router": "^3.0.2",
    "redux": "^3.0.4",
    "redux-async": "^2.0.1",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-isomorphic-tools": "^2.6.6"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "0.0.14",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "rimraf": "^2.5.4",
    "stats-webpack-plugin": "^0.4.3",
    "style-loader": "^0.13.1"
  }
}
